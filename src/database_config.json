{
    "grouped-daily-pipeline": {
        "name": "GROUPED_DAILY",
        "fields_mapping": {
            "date": ["date", "DATE"],
            "T": ["exchange_symbol", "VARCHAR(255)"],
            "v": ["trading_volume", "FLOAT"],
            "vw": ["volume_weighted_avg", "FLOAT"],
            "o": ["open_price", "FLOAT"],
            "c": ["close_price", "FLOAT"],
            "h": ["highest_price", "FLOAT"],
            "l": ["lowest_price", "FLOAT"],
            "t": ["end_window", "DATETIME"],
            "n": ["n_transaction", "INTEGER"],
            "updated_at": ["updated_at", "DATETIME"]
        },
        "required_fields": ["date", "T", "o", "c", "h", "l", "t", "updated_at"]
    },
    "ticker-basic-details-pipeline": {
        "name": "TICKER_BASIC_DETAILS",
        "fields_mapping": {
            "active": ["active", "BOOLEAN"],
            "cik": ["cik", "VARCHAR(255)"],
            "composite_figi": ["composite_figi", "VARCHAR(255)"],
            "currency_name": ["currency_name", "VARCHAR(255)"],
            "c": ["last_updated_utc", "DATETIME"],
            "locale": ["locale", "VARCHAR(255)"],
            "market": ["market", "VARCHAR(255)"],
            "name": ["name", "VARCHAR(255)"],
            "primary_exchange": ["primary_exchange", "VARCHAR(255)"],
            "share_class_figi": ["share_class_figi", "VARCHAR(255)"],
            "ticker": ["exchange_symbol", "VARCHAR(255)"],
            "type": ["type", "VARCHAR(255)"],
            "updated_at": ["updated_at", "DATETIME"]
        },
        "required_fields": ["ticker", "market", "updated_at"]
    },
    "sp500-basic-details-pipeline": {
        "name": "SP500_BASIC_DETAILS",
        "fields_mapping": {
            "Symbol": ["exchange_symbol",  "VARCHAR(255)"],
            "Security": ["name", "VARCHAR(255)"],
            "GICS Sector": ["sector", "VARCHAR(255)"],
            "GICS Sub-Industry": ["sub_industry", "VARCHAR(255)"],
            "Headquarters Location": ["hq_location", "VARCHAR(255)"],
            "Date added": ["date_added", "DATETIME"],
            "CIK": ["cik", "VARCHAR(255)"],
            "Founded": ["founded", "VARCHAR(255)"],
            "url": ["url", "VARCHAR(255)"],
            "updated_at": ["updated_at", "DATETIME"]
        },
        "required_fields": ["Symbol", "Security", "GICS Sector","CIK", "updated_at"]
    },
    "indexes-daily-close-pipeline": {
        "name": "INDEXES_DAILY_CLOSE",
        "fields_mapping": {
            "date": ["date", "DATE"],
            "index": ["index_code",  "VARCHAR(255)"],
            "value": ["close_value", "FLOAT"],
            "updated_at": ["updated_at", "DATETIME"]
        },
        "required_fields": ["date","index", "updated_at"]
    }
}
